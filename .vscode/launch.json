    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/viewerbuild/build/opendlv-logic-cfsd18-sensation-slam",
            "args": ["--cid=118", "--id=120", "--detectConeId=116", "--estimationId=112", "--gatheringTimeMs=20", "--sameConeThreshold=2", "--refLatitude=57.71395044", "--refLongitude=11.94875904", "--timeBetweenKeyframes=150", "--coneMappingThreshold=7", "--conesPerPacket=20"],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}